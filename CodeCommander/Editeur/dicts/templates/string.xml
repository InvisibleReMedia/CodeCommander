													<variable>siVide</variable>
													<expression>false</expression>
												</affectation>
											</code>
										</coding>
										<coding name="false" indent="3">
											<code>
												<affectation indent="4">
													<variable>siVide</variable>
													<expression>true</expression>
												</affectation>
											</code>
										</coding>
									</codings>
								</usetemplate>
							</code>
						</coding>
						<coding name="increment" indent="1">
